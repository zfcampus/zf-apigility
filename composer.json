{
    "name": "zfcampus/zf-apigility",
    "description": "ZF Apigility module by ZF",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "zf2"
    ],
    "homepage": "http://framework.zend.com/",
    "minimum-stability": "dev",
    "repositories": [
        { "type": "composer", "url": "https://packages.zendframework.com/" },
        { "type": "composer", "url": "http://packages-zfcampus.rhcloud.com/" }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "dev-develop",
        "zfcampus/zf-rpc": "dev-master",
        "zfcampus/zf-content-negotiation": "dev-master",
        "zfcampus/zf-versioning": "dev-master",
        "zfcampus/zf-rest": "dev-master",
        "zfcampus/zf-api-problem": "dev-master",
        "zfcampus/zf-hal": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "ZF\\Apigility\\": "src/",
            "ZFTest\\": "test/"
        }
    }
}
