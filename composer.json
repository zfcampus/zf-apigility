{
    "name": "zfcampus/zf-apigility",
    "description": "Apigility module for ZF2",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "api",
        "apigility",
        "framework",
        "zf2"
    ],
    "homepage": "http://apigility.org/",
    "support": {
        "email": "apigility-users@zend.com",
        "irc": "irc://irc.freenode.net/apigility",
        "source": "https://github.com/zfcampus/zf-apigility",
        "issues": "https://github.com/zfcampus/zf-apigility/issues"
    },
    "minimum-stability": "dev",
    "repositories": [
        { "type": "composer", "url": "https://packages.zendframework.com/" }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.3.*",
        "zfcampus/zf-rpc": "*",
        "zfcampus/zf-content-negotiation": "*",
        "zfcampus/zf-versioning": "*",
        "zfcampus/zf-rest": "*",
        "zfcampus/zf-api-problem": "*",
        "zfcampus/zf-hal": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "ZF\\Apigility\\": "src/",
            "ZFTest\\": "test/"
        }
    }
}
